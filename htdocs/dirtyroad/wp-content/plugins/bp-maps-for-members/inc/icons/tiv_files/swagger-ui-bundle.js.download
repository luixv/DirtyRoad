            <div id="app" v-cloak>
                <input type="hidden" id="username" name="username" v-model="login.username">
                
                
                <input type="hidden" id="pingResume" name="pingResume" value="/as/t3Phy/resume/as/authorization.ping">
                
                
                
                
                <transition v-on:after-enter="afterEnter" mode="out-in" name="fade">
                    <div key="1" v-if="step === 1">
                        
                        <h1 style="visibility: hidden">
                            <span class="badge badge-pill badge-dark">
                                <span class="icon-basic-person-2"></span>
                                <button type="button" class="close-icon" aria-label="Close">&times;</button>
                            </span>
                        </h1>
                        <label for="userid" class="sr-only">User ID</label>
                        <div class="form-group label-wrapper">
                            <input maxlength="20" id="userid" type="text" name="userid" class="form-control"
                                   v-bind:class="{'is-invalid':login.usernameEmpty}"
                                   ref="inputField" v-model="login.userid" autocomplete="username"
                                   autofocus/>
                            <div class="label-text"><span>User ID</span></div>
                            <small v-if="login.usernameEmpty===true" class="text-danger float-left"
                                   id="error-empty">Please provide your UserID</small>
                        </div>
                        <div class="form-check-inline float-left pb-3"
                             style="visibility: hidden">
                            <div class="onoffswitch">
                                <input type="checkbox" v-model="login.rememberme" name="dummy"
                                       class="onoffswitch-checkbox" id="dummy" ref="switchRememberMe">
                                <label class="onoffswitch-label" for="dummy">
                                    <span class="onoffswitch-inner"></span>
                                    <span class="onoffswitch-switch"></span>
                                </label>

                            </div>
                            <span class="ml-2">Remember me</span>
                        </div>
                        <button id="next-btn" @click.prevent="next()" class="btn btn-lg btn-primary btn-block">Next</button>
                    </div>
                    <div key="2" v-if="step === 2">
                        <h1>
                            <span class="badge badge-pill badge-dark">
                                <span class="icon-basic-person-2"></span>
                                <span> {{login.username}}</span>
                                <button id="username_back_btn" type="button" @click.prevent="back()" class="close-icon"
                                        aria-label="Close">&times;</button>
                            </span>
                        </h1>
                        <label for="password" class="sr-only">Password</label>
                        <div class="form-group label-wrapper">
                            <input id="password" type="password" name="password" ref="inputField" class="form-control"
                                   autocomplete="current-password" required
                                   v-model="login.password"/>
                            <div class="label-text"><span>Password</span></div>
                        </div>
                        
                        <div class="form-check-inline float-left pb-3"
                             style="visibility:visible">
                            <div class="onoffswitch">
                                <input type="checkbox" v-model="login.rememberme" name="onoffswitch"
                                       class="onoffswitch-checkbox" id="rememberme" ref="switchRememberMe">
                                <label class="onoffswitch-label" for="rememberme">
                                    <span class="onoffswitch-inner"></span>
                                    <span class="onoffswitch-switch"></span>
                                </label>

                            </div>
                            <span class="ml-2">Remember me</span>
                        </div>
                        <button id="loginSubmitButton" class="btn btn-lg btn-primary btn-block" type="submit">
                            <span id="loadingSpinner" class="spinner-border" role="status" aria-hidden="true"></span>
                            Log on
                        </button>
                    </div>
                </transition>
                <div class="row">
                    <div class="col-5 text-right p-0">
                        <small class="form-text text-muted mr-n4">
                            <a href="https://login.daimler.com/password/cd/modify" target="_blank"
                               class="password-change">Modify password</a>
                        </small>
                    </div>
                    <div class="col-2 p-0">
                        <small class="form-text text-muted">
                            <span>|</span>
                        </small>
                    </div>
                    <div class="col-5 text-left p-0">
                        <small class="form-text text-muted ml-n4">
                            <a href="https://login.daimler.com/password/reset" target="_blank"
                               class="password-reset">Forgot password?</a>
                        </small>
                    </div>
                </div>
            </div>
